{
  "name": "mcom-api",
  "version": "0.0.3",
  "description": "Mindfulness.com Typescript graphql api.",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm-run-all -p start:server start:linting start:codegen",
    "start:server": "nodemon",
    "start:linting": "chokidar \"src\" -d 5000 -t 15000 -c \"npm run lint\"",
    "start:tests": "chokidar \"src\" -d 5000 -t 15000 -c \"jest -o\"",
    "start:codegen": "chokidar \"src/**/*!(spec).gql\" -d 1000 -t 0 -c \"npm run codegen\"",
    "setup": "npm i && npm run env:setup",
    "env:setup": "cp .env-template .env",
    "db:drop": "dropdb mcom",
    "db:create": "createdb mcom",
    "db:sync": "ts-node --files ./db-ops/sync && npm run db:migrate",
    "db:reset": "sequelize db:seed:undo:all && FORCE=true npm run db:sync",
    "db:setup": "npm run db:sync",
    "db:seed": "npm run build:migrations && sequelize db:seed:all",
    "db:migrate": "npm run build:migrations && sequelize db:migrate",
    "db:migrate:undo": "npm run build:migrations && sequelize db:migrate:undo",
    "db:migrate:generate": "sequelize migration:generate --name unnamed",
    "test": "jest --maxWorkers=2",
    "serve": "node ./dist/src/index.js",
    "build": "gulp build",
    "build:migrations": "tsc ./db-ops/**/*.ts",
    "deploy": "gulp deploy",
    "lint": "eslint -c ./.eslintrc \"src/**/*.ts\"",
    "codegen": "gql-gen --config graphql-gen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mindfulness-com/mcom-api.git"
  },
  "keywords": [
    "typescript",
    "graphql",
    "node",
    "api"
  ],
  "engines": {
    "node": ">=10.15.0"
  },
  "author": "Mindfulness.com",
  "license": "ISC",
  "dependencies": {
    "@okgrow/graphql-scalars": "^0.4.5",
    "@sendgrid/mail": "^6.4.0",
    "@sentry/node": "^5.2.0",
    "analytics-node": "^3.3.0",
    "apollo-server-express": "^2.4.2",
    "aws-sdk": "^2.429.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.4",
    "change-case": "^3.1.0",
    "dataloader": "^1.4.0",
    "date-fns": "^1.30.1",
    "date-fns-timezone": "^0.1.4",
    "dotenv": "^6.2.0",
    "duration-to-seconds": "^1.0.0-rc.2",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "graphql-fields": "^2.0.3",
    "graphql-middleware": "^3.0.2",
    "graphql-middleware-sentry": "^3.1.1",
    "graphql-type-json": "^0.2.4",
    "graphql-voyager": "^1.0.0-rc.26",
    "humanparser": "^1.10.0",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.11",
    "nanoid": "^2.0.1",
    "parse-bearer-token": "^1.0.1",
    "pg": "^7.8.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "sequelize-typescript": "^0.6.7",
    "timezone-support": "^1.8.1",
    "to-time": "^1.0.2",
    "util": "^0.11.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/analytics-node": "^3.1.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.26",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.0.7",
    "@types/graphql-fields": "^1.3.0",
    "@types/graphql-type-json": "^0.1.3",
    "@types/gulp": "^4.0.6",
    "@types/gulp-replace": "0.0.31",
    "@types/humanparser": "^1.1.1",
    "@types/jest": "^24.0.9",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.121",
    "@types/mock-require": "^2.0.0",
    "@types/nanoid": "^1.2.1",
    "@types/node-schedule": "^1.2.3",
    "@types/sequelize": "^4.27.37",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "chokidar-cli": "^1.2.2",
    "eslint": "^5.14.1",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "graphql-code-generator": "^0.16.1",
    "graphql-codegen-add": "^0.16.1",
    "graphql-codegen-introspection": "^0.16.1",
    "graphql-codegen-typescript-common": "0.16.1",
    "graphql-codegen-typescript-resolvers": "^0.16.1",
    "graphql-codegen-typescript-server": "^0.16.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-replace": "^1.0.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  }
}
