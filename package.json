{
  "name": "@mindfulness/utils",
  "version": "0.1.28",
  "description": "Mindfulness.com shared typescript utils",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start:linting": "chokidar \"src\" -d 5000 -t 15000 -c \"npm run lint\"",
    "start:tests": "chokidar \"src\" -d 5000 -t 15000 -c \"jest -o\"",
    "build": "tsc",
    "publish": "chmod 755 ./publish.sh && ./publish.sh",
    "postinstall": "[ -d './src' ] || cp -rf ./dist/* ./",
    "test": "TZ=UTC jest --maxWorkers=2",
    "lint": "eslint -c ./.eslintrc \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mindfulness-com/mcom-utils.git"
  },
  "keywords": [
    "typescript",
    "graphql",
    "node",
    "api"
  ],
  "engines": {
    "node": ">=10.15.0"
  },
  "author": "Mindfulness.com",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.4",
    "change-case": "^3.1.0",
    "compare-versions": "^3.6.0",
    "date-fns": "^1.30.1",
    "date-fns-timezone": "^0.1.4",
    "dotenv": "^6.2.0",
    "duration-to-seconds": "^1.0.0-rc.2",
    "humanparser": "^1.10.0",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.15",
    "memoizee": "^0.4.14",
    "microseconds": "^0.2.0",
    "nanoid": "^2.0.1",
    "parse-bearer-token": "^1.0.1",
    "pg-escape": "^0.2.0",
    "query-string": "^7.0.1",
    "timezone-support": "^1.8.1",
    "to-time": "^1.0.2",
    "util": "^0.11.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/dotenv": "^6.1.0",
    "@types/gulp": "^4.0.6",
    "@types/gulp-replace": "0.0.31",
    "@types/humanparser": "^1.1.1",
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.149",
    "@types/memoizee": "^0.4.3",
    "@types/mock-require": "^2.0.0",
    "@types/nanoid": "^1.2.1",
    "@types/pg-escape": "^0.2.0",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "chokidar-cli": "^1.2.2",
    "eslint": "^6.8.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.0.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-replace": "^1.0.0",
    "jest": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-deferred": "^1.0.4",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.7.5"
  }
}
