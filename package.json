{
  "name": "@mindfulness/utils",
  "version": "0.1.76",
  "description": "Mindfulness.com shared typescript utils",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start:linting": "chokidar \"src\" -d 5000 -t 15000 -c \"npm run lint\"",
    "start:tests": "chokidar \"src\" -d 5000 -t 15000 -c \"jest -o\"",
    "build": "tsc && npm run doc",
    "prepublishOnly": "chmod 755 ./publish.sh && ./publish.sh",
    "postinstall": "[ -d './src' ] || cp -rf ./dist/* ./",
    "test": "TZ=UTC jest --maxWorkers=2",
    "test:one": "TZ=UTC jest --watch",
    "lint": "eslint -c ./.eslintrc \"src/**/*.ts\"",
    "lint:fix": "eslint -c ./.eslintrc \"src/**/*.ts\" --fix",
    "doc": "typedoc ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mindfulness-com/mcom-utils.git"
  },
  "keywords": [
    "typescript",
    "graphql",
    "node",
    "api"
  ],
  "engines": {
    "node": ">=16"
  },
  "author": "Mindfulness.com",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.7.2",
    "change-case": "^4.1.2",
    "compare-versions": "^6.0.0",
    "date-fns": "^1.30.1",
    "date-fns-timezone": "^0.1.4",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "microseconds": "^0.2.0",
    "nanoid": "^3.3.6",
    "pg-escape": "^0.2.0",
    "query-string": "^7.1.3",
    "timezone-support": "^3.1.0",
    "to-time": "^1.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.38",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.196",
    "@types/memoizee": "^0.4.8",
    "@types/microseconds": "^0.2.0",
    "@types/mock-require": "^2.0.1",
    "@types/pg-escape": "^0.2.1",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.1",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "ts-deferred": "^1.0.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  }
}
